import { {{Entity}} } from "..";

it("should return a json object", () => {
  const now = new Date();
  const dto = {
    id: "some-entity-id",
    title: "Some title",
    createdAt: now.toISOString(),
    updatedAt: now.toISOString(),
  };

  const entity = {{Entity}}.create(dto);
  const json = entity.jsonify();
  expect(json).toEqual(dto);
});